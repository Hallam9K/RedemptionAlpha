		Configs: {
			RedeConfigClient: {
				DisplayName: Redemption config. del Cliente

				NoBossIntroText: {
					Label: Desactivar titulos de jefes
					Tooltip: Desactiva el texto de introducción al estilo 'The Legend of Zelda' de los jefes.
				}

				CameraLockDisable: {
					Label: Desactivar bloqueo de cámara
					Tooltip:
						'''
						Desactiva el bloqueo de la cámara durante las escenas, junto con la invencibilidad y efectos de viñeta
						No recomendado, Por lo menos la primera vez jugando ya que puede arruinar la experiencia.
						'''
				}

				ShakeIntensity: {
					Label: Intensidad de la vibracion de pantalla
					Tooltip: Reduce la intensidad de los efectos de vibracion, en 0 se desactivara completamente
				}

				ElementalDamageTooltipStyle: {
					Label: Estilo de descripción de herramienta de daño elemental
					Tooltip: Cambia la forma en que aparecen los aumentos de daño elemental en las descripciones emergentes de las armas, o también puedes desactivarlos
				}

				NoFlyBuzz: {
					Label: Desactivar el zumbido de las moscas
					Tooltip: Las moscas se vuelven silenciosas
				}

				DialogueInChat: {
					Label: Guardar diálogo de cinemática en el chat
					Tooltip: El diálogo de las cinemáticas, el Cáliz de Alineación y los Jefes también se enviará al chat de texto.
				}

				LegacyNebSky: {
					Label: Legado de Nebuleus Sky
					Tooltip: Revierte la forma final de nebuleus sky a la antigua imagen rotante
				}

				DisableMPWarning: {
					Label: Deshabilitar advertencia multijugador
					Tooltip: Desactiva el mensaje de advertencia que se muestra en el modo multijugador
				}
			}

			RedeConfigServer: {
				DisplayName: Configuración global

				DialogueWaitTime: {
					Label: Tiempo Extra de Espera de Dialogo
					Tooltip:
						'''
						Aumenta la duración del texto de diálogo que hay antes de pasar a otro, auméntalo si eres un lector lento.
						Valor en segundos.
						'''
				}

				DialogueSpeed: {
					Label: Rapidez del dialogo
					Tooltip:
						'''
						Solo se recomienda cambiar si el dialogo va muy lento o muy rapido.
						Este es el tiempo medido en segundos para que aparezca cada letra, más alto = más rápido.
						Cuanto más rápido lo hagas, más difícil será leerlo antes de terminar, así que ajusta también el Tiempo Extra de Espera del Diálogo
						'''
				}

				ElementDisable: {
					Label: Desactivar elementos
					Tooltip: Desactiva la resistencia elemental y daño
				}

				VanillaGuardPointsDisable: {
					Label: Desactivar la mecánica del 'blindaje' en enemigos Vanilla
					Tooltip: Desactiva el 'blindaje', una mecánica añadida por el Mod of Redemption
				}

				NoPZBuildUp: {
					Label: Sin espera del Paciente Cero
					Tooltip: Hace que la pelea empieze inmediatamente(paciente cero), mayormente para los no-hitters. (Provocará que el efecto de pulso no se sincronice bien con la música)
				}

				FindNewStructureCoords: {
					Label: Restablecer coordenadas de estructuras
					Tooltip:
						'''
						Al ingresar al mundo, el juego buscará bloques importantes de estructuras para establecer sus coordenadas
						Solo es necesario si desea reubicar estructuras, por ejemplo, destruir el portal de superficie y colocar el marco del portal en otro lugar
						Mientras está desactivado, las coordenadas se establecen y no se cambiarán
						'''
				}

				FunniHeader: Lo chistosito (¡Recuerda deshabilitarlos antes de querer crear un mundo normal! Activalos solo si sabes lo que haces)

				FunniSpiders: {
					Label: ¡Oops! ¡Arañas!
					Tooltip: Generar un nuevo mundo causará que todos los bloques de piedra se conviertan en piedra infestada
				}

				FunniWasteland: {
					Label: Okey, ¿quien activo la bomba nuclear?
					Tooltip: Generar un nuevo mundo siempre hará que un bioma de Páramo se genere naturalmente (Esto puede romper la progresión, obviamente)
				}

				FunniAllWasteland: {
					Label: Liden en casa:
					Tooltip: Generar un nuevo mundo convertirá todos los bloques en su versión irradiada (Esto puede romper la progresión, obviamente)
				}

				FunniJanitor: {
					Label: Última voluntad del Conserje
					Tooltip: Generar un nuevo mundo creará un laboratorio limpio y 'seguro' (Esto puede romper la progresión, obviamente)
				}

				FunniAncient: {
					Label: Mundo antiguo
					Tooltip: Generar un nuevo mundo convertirá todos los bloques en su versión antigua o gáthica, y las tumbas gáthicas se generarán más frecuentemente.
				}
